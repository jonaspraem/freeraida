{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "freeraida-client": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "client",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "public/js/app",
            "deployUrl": "/js/app/browser/",
            "index": "client/index.html",
            "browser": "client/app/main.ts",
            "tsConfig": "client/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "client/images",
                "output": "images"
              }
            ],
            "styles": ["client/styles/styles.scss"],
            "stylePreprocessorOptions": {
              "sass": {
                "silenceDeprecations": [
                  "legacy-js-api",
                  "import",
                  "slash-div",
                  "global-builtin",
                  "color-functions",
                  "if-function",
                  "abs-percent"
                ]
              }
            },
            "loader": {
              ".png": "file",
              ".jpg": "file",
              ".jpeg": "file",
              ".gif": "file",
              ".svg": "file"
            },
            "scripts": [],
            "outputHashing": "none"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "6mb"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "freeraida-client:build:production"
            },
            "development": {
              "buildTarget": "freeraida-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
