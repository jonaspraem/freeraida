{
  "name": "freeraida",
  "license": "MIT",
  "version": "1.1.5",
  "private": true,
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "start": "del-cli server/dist && tsc && node ./server/dist/www",
    "server": "npm run server:clean && npm run server:once && concurrently -k \"npm:server:build\" \"npm:server:start\"",
    "server:clean": "del-cli server/dist",
    "server:once": "tsc",
    "server:build": "tsc --watch --preserveWatchOutput",
    "server:start": "nodemon --delay 200ms --watch server/dist --ext js --exec \"node ./server/dist/www\"",
    "node-build": "tsc",
    "build": "del-cli public/js/app && ng build freeraida-client --configuration development --watch",
    "build:prod": "del-cli public/js/app && ng build freeraida-client --configuration production",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js,.html",
    "heroku-prebuild": "npm install --include=dev",
    "heroku-postbuild": "npm run build:prod"
  },
